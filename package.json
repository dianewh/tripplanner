{
  "name": "yogaclassbuilder",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setuptests.js",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules",
    "collectCoverageFrom": [
      "client/src/components/*.{jsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon server/index.js"
  },
  "keywords": [],
  "author": "Diane Huang",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^6.0.3",
    "style-loader": "^0.22.1",
    "supertest": "^3.1.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "express": "^4.16.3",
    "nodemon": "^1.18.3",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "directories": {
    "test": "tests"
  }
}
